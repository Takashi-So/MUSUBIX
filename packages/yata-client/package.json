  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./knowledge": {
      "import": "./dist/knowledge/index.js",
      "types": "./dist/knowledge/index.d.ts"
    },
    "./reasoning": {
      "import": "./dist/reasoning/index.js",
      "types": "./dist/reasoning/index.d.ts"
    },

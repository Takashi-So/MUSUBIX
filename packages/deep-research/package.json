{
  "name": "@nahisaho/musubix-deep-research",
  "version": "3.4.6",
  "description": "Deep Research Integration - Iterative search-read-reason cycle for AI agents",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    },
    "./test": {
      "types": "./dist/test/index.d.ts",
      "import": "./dist/test/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -b",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "deep-research",
    "ai-agent",
    "search",
    "reasoning",
    "knowledge-graph",
    "musubix"
  ],
  "author": "nahisaho",
  "license": "MIT",
  "dependencies": {
    "@nahisaho/musubix-core": "^3.3.10",
    "@nahisaho/musubix-expert-delegation": "^3.2.0",
    "@nahisaho/musubix-neural-search": "^2.2.0",
    "@nahisaho/musubix-agent-orchestrator": "^2.4.0",
    "@nahisaho/musubix-workflow-engine": "^2.4.0",
    "@musubix/knowledge": "^3.0.0",
    "axios": "^1.6.0",
    "dompurify": "^3.0.6",
    "jsdom": "^23.0.0"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "vscode": "*"
  },
  "peerDependenciesMeta": {
    "vscode": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}

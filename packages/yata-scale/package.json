{
  "name": "@nahisaho/yata-scale",
  "version": "2.2.0",
  "description": "YATA Knowledge Graph Scale-up Package - Distributed graph, high-performance indexing, multi-level cache",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./shard": {
      "types": "./dist/shard/index.d.ts",
      "import": "./dist/shard/index.js"
    },
    "./index": {
      "types": "./dist/index/index.d.ts",
      "import": "./dist/index/index.js"
    },
    "./cache": {
      "types": "./dist/cache/index.d.ts",
      "import": "./dist/cache/index.js"
    },
    "./query": {
      "types": "./dist/query/index.d.ts",
      "import": "./dist/query/index.js"
    },
    "./sync": {
      "types": "./dist/sync/index.d.ts",
      "import": "./dist/sync/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "yata",
    "knowledge-graph",
    "scale",
    "sharding",
    "distributed",
    "cache",
    "index",
    "musubix"
  ],
  "author": "nahisaho",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nahisaho/MUSUBIX.git",
    "directory": "packages/yata-scale"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "neverthrow": "^8.2.0",
    "lmdb": "^3.2.6",
    "xxhash-wasm": "^1.1.0",
    "lru-cache": "^11.0.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  },
  "peerDependencies": {
    "@nahisaho/yata-local": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@nahisaho/yata-local": {
      "optional": true
    }
  }
}

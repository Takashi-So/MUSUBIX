{
  "name": "@nahisaho/musubix-expert-delegation",
  "version": "3.4.0",
  "description": "Expert Delegation System for MUSUBIX - 7 Specialized AI Experts with VS Code LM API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./experts": {
      "import": "./dist/experts/index.js",
      "types": "./dist/experts/index.d.ts"
    },
    "./triggers": {
      "import": "./dist/triggers/index.js",
      "types": "./dist/triggers/index.d.ts"
    },
    "./delegation": {
      "import": "./dist/delegation/index.js",
      "types": "./dist/delegation/index.d.ts"
    },
    "./providers": {
      "import": "./dist/providers/index.js",
      "types": "./dist/providers/index.d.ts"
    },
    "./mcp": {
      "import": "./dist/mcp/index.js",
      "types": "./dist/mcp/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "musubix",
    "expert-delegation",
    "ai-agents",
    "vscode",
    "mcp",
    "ears",
    "formal-verification",
    "ontology"
  ],
  "author": "nahisaho",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nahisaho/musubix.git",
    "directory": "packages/expert-delegation"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@nahisaho/musubix-core": "workspace:*",
    "@nahisaho/musubix-formal-verify": "workspace:*",
    "@nahisaho/musubix-ontology-mcp": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.85.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "peerDependencies": {
    "vscode": "^1.85.0"
  },
  "peerDependenciesMeta": {
    "vscode": {
      "optional": true
    }
  }
}
